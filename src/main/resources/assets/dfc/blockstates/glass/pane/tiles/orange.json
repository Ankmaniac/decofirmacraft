{
  "multipart": [
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_post"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_noside"
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_noside_alt"
      },
      "when": {
        "east": "false"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_noside_alt",
        "y": 90
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "dfc:block/glass/pane/tiles/orange_noside",
        "y": 270
      },
      "when": {
        "west": "false"
      }
    }
  ]
}